<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::marketing">
                    <resource id="Magenest_GoogleShopping::google_shopping" title="Google Shopping" sortOrder="60">
                        <resource id="Magenest_GoogleShopping::google_product_template" title="Product Template"/>
                        <resource id="Magenest_GoogleShopping::google_category_template" title="Google Category Mapping"/>
                        <resource id="Magenest_GoogleShopping::google_feed" title="Google Feed"/>
                        <resource id="Magenest_GoogleShopping::cron_task" title="Cron Schedule List"/>
                        <resource id="Magenest_GoogleShopping::configuration" title="Configuration" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_GoogleShopping::config_googleshopping" title="Config Google Shopping"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
